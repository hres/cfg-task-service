{
	"realm" : "cfg-classification",
	"resource" : "cfg-task-service",
	"auth-server-url" : "https://keycloak.canadaeast.cloudapp.azure.com/auth",
	"ssl-required" : "all",
	"use-resource-role-mappings" : false,
	"enable-cors" : true,
	"bearer-only" : true,
	"autodetect-bearer-only" : true,
	"enable-basic-auth" : false,
	"expose-token" : false,
	"allow-any-hostname" : false,
	"truststore": "/etc/tomcat8/ssl/tomcat.keystore",
	"truststore-password": "password",
	"token-minimum-time-to-live" : 10,
	"min-time-between-jwks-requests" : 10,
	"public-key-cache-ttl": 86400,
	"public-client":true
}
